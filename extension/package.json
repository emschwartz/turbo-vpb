{
  "name": "turbovpb-extension",
  "version": "1.0.0",
  "description": "Turbocharge phone banking with OpenVPB, VAN, and BlueVote. Call and text with 2 clicks!",
  "scripts": {
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension",
    "build": "rm -rf dist && parcel build src/manifest.json --config @parcel/config-webextension"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emschwartz/turbo-vpb.git"
  },
  "author": "Evan Schwartz <evan@turbovpb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emschwartz/turbo-vpb/issues"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "homepage": "https://github.com/emschwartz/turbo-vpb#readme",
  "devDependencies": {
    "@parcel/config-webextension": "^2.9.3",
    "@tailwindcss/forms": "^0.5.3",
    "@types/webextension-polyfill": "0.10.1",
    "parcel": "^2.9.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^2.0.11",
    "@preact/signals": "^1.1.1",
    "content-scripts-register-polyfill": "^4.0.2",
    "preact": "^10.11.0",
    "react-qr-code": "^2.0.7",
    "reconnecting-websocket": "^4.4.0",
    "tingle.js": "^0.16.0",
    "webextension-polyfill": "^0.10.0"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime",
    "react/jsx-dev-runtime": "preact/jsx-runtime",
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  },
  "@parcel/bundler-default": {
    "minBundles": 10000000,
    "minBundleSize": 3000,
    "maxParallelRequests": 20
  }
}